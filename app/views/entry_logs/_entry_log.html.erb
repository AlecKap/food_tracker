<div id="<%= dom_id entry_log %>">
  <p>
    <strong>Date:</strong>
    <%= entry_log.date %>
  </p>

  <p>
    <strong>Type:</strong>
    <%= entry_log.type %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= entry_log.name %>
  </p>

  <p>
    <strong>Calories:</strong>
    <%= entry_log.calories %>
  </p>

  <p>
    <strong>Protein:</strong>
    <%= entry_log.protein %>
  </p>

  <p>
    <strong>Added sugars:</strong>
    <%= entry_log.added_sugars %>
  </p>

  <p>
    <strong>Total sugars:</strong>
    <%= entry_log.total_sugars %>
  </p>

  <p>
    <strong>Carbs:</strong>
    <%= entry_log.carbs %>
  </p>

</div>
